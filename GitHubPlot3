step = 1;
bins = min(data):step:max(data);
dist = hist(data, bins);

ndist = dist / sum(dist);

cdist = cumsum(ndist);
plot(bins, cdist);
grid on
ylim([0 1])
xlim([21000 26000])
xticks([21000:1000:26000])
xlabel('Annual AC Energy (MWh)') 
ylabel('Cumulative Probability') 

%histogram(data, bins)

